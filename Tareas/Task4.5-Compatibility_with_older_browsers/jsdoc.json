{
    "source":{
        "include": ["Task4-5-Compatibility_with_older_browsers.js"],
        "exclude": ["node_modules", "docs"],
        "includePattern": ".*\\.js$",
        "excludePattern": ".*\\.test\\.js$"
    },

    "opts":{
        "destination": "./docs",
        "template": "node_modules/minami",
        "recurse": true
    },

    "templates": {
